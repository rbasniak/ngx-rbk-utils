{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "rbk",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "rbk",
      "kebab-case"
    ],
    "curly": [true, "ignore-same-line"],
    "one-line": [true,
        "check-catch",
        "check-finally",
        "check-open-brace",
        "check-whitespace"
    ],
    "eofline": false,
    "no-namespace": false,
    "align": false,
    "max-line-length": [
      true,
      160
    ],
    "no-empty-interface": false
  }
}
